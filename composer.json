{
    "name": "biggo6/laravel-updater",
    "description": "A Laravel 5.2+ self updater package.",
    "keywords": ["framework", "laravel", "self-update", "update"],
    "license": "MIT",
    "authors": [
        {
            "name": "Joram Kimata",
            "email": "joramkimata@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "illuminate/support": "4.2.*",
        "naneau/semver": "*"
    },
    "autoload": {
        "classmap": [
            "src/migrations",
            "src/controllers"
        ],
        "psr-0": {
            "Biggo6\\LaravelUpdater\\": "src/"
        }
    },
     "support": {
        "issues": "https://github.com/biggo6/laravel-updater/issues",
        "source": "https://github.com/biggo6/laravel-updater",
        "wiki": "https://github.com/biggo6/laravel-updater/wiki"
    }
}
